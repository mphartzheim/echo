{
  "name": "echo-location-wx",
  "version": "1.1.0",
  "main": "src/main.mjs",
  "type": "module",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.example.echolocationwx",
    "productName": "Echo Location WX",
    "files": [
      "**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "dependencies": {
    "axios": "^1.6.8"
  },
  "devDependencies": {
    "electron": "^35.1.4",
    "electron-builder": "^26.0.12"
  }
}